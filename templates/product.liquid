      document.getElementById('bat_3_price').innerHTML = formatPrice(bat_price_3 - bat_price_selected);
      document.getElementById('bat_4_price').innerHTML = formatPrice(bat_price_4 - bat_price_selected);
      document.getElementById('bat_5_price').innerHTML = formatPrice(bat_price_5 - bat_price_selected);
      document.getElementById('bat_6_price').innerHTML = formatPrice(bat_price_6 - bat_price_selected);

      // document.getElementById('omv_1_price').innerHTML = formatPrice(omv_price_1 - omv_price_selected);
      document.getElementById('omv_2_price').innerHTML = formatPrice(omv_price_2 - omv_price_selected);

      let price_totaal = omv_price_selected + bat_price_selected + installatie_price;
      let aanbetaling;

      switch (aanbetalingValue) {
        case 'aanbetaling_optie1':
          korting = 250;
          price_totaal_korting = price_totaal - korting;
          aanbetaling = price_totaal_korting;
          betaal_text = `€ ${Math.abs(aanbetaling).toFixed(2).replace('.', ',')}` + '</span>';
          break;
        case 'aanbetaling_optie2':
          korting = 100;
          price_totaal_korting = price_totaal - korting;
          aanbetaling = price_totaal_korting * 0.4;
          betaal_text = `€ ${Math.abs(aanbetaling).toFixed(2).replace('.', ',')}` + '</span><span class="text-span-13"> van de € ' + `${Math.abs(price_totaal_korting).toFixed(2).replace('.', ',')}` + ' </span>';
          break;
        default:
          korting = 0;
          price_totaal_korting = price_totaal - korting;
          aanbetaling = 99;
          betaal_text = `€ ${Math.abs(aanbetaling).toFixed(2).replace('.', ',')}` + '</span><span class="text-span-13"> van de € ' + `${Math.abs(price_totaal_korting).toFixed(2).replace('.', ',')}` + ' </span>';
          break;
      }

      document.getElementById('final_omv_price').innerHTML = formatPrice(omv_price_selected).replace(/\+/g, '');
      document.getElementById('final_bat_price').innerHTML = formatPrice(bat_price_selected).replace(/\+/g, '');
