      }

      if (!omvormerValue) {
        document.getElementById('omvormerAlert').style.display = 'block';
        smoothScroll('omvormerAlert');
        errorpage = true;
      }

      if (!errorpage) {
        return true;
      }else{
        return false;
      }
    }

    function togglePage() {
      var page1 = document.getElementById('page_1');
      var page2 = document.getElementById('page_2');
      if (page1.style.display === 'none') { //terug naar configure
        page1.style.display = 'block';
        page2.style.display = 'none';
        document.getElementById('showonpage2').style.display = 'none';
        document.getElementById('hideonpage2a').style.display = 'block';
        document.getElementById('hideonpage2b').style.display = 'block';
      } else { //vooruit naar bestellen
        page1.style.display = 'none';
        page2.style.display = 'block';
        document.getElementById('showonpage2').style.display = 'block';
        document.getElementById('hideonpage2a').style.display = 'none';
        document.getElementById('hideonpage2b').style.display = 'none';
        smoothScroll('toptop');
      }
    }

    function submitForm() {
      let errorFound = false;

      const voorwaardenCheckbox = document.getElementById("input-check-voorwaarden");
      const beleidCheckbox = document.getElementById("input-check-beleid");

      if (!(voorwaardenCheckbox.checked && beleidCheckbox.checked)) {
        errorFound = true;
        document.getElementById('errorText').innerHTML = 'Je moet akkoord gaan met onze voorwaarden om een bestelling te plaatsen.';
      }

      //postcode valid check
      let postcode = document.getElementById('input-postcode').value;
      let postcodeRemoveSpaces = postcode.replace(/\s/g, '');
      let postcodeUpper = postcodeRemoveSpaces.toUpperCase();
      const postalCodePattern = /^[1-9][0-9]{3}[A-Z]{2}$/;

      if (!postalCodePattern.test(postcodeUpper)) {
        document.getElementById('errorText').innerHTML = 'Vul een geldige postcode in.';
        errorFound = true;
      } 

      //emailcheck
      if (!isEmail(document.getElementById('input-email').value)) {
        document.getElementById('errorText').innerHTML = 'Vul een geldig email-adres in.';
        errorFound = true;
      } 


      //input check
      const ids = ['input-naam', 'input-telefoon', 'input-huisnr'];
      const zakelijkIds = ['input-bedrijfsnaam', 'input-kvk', 'input-btw'];

      ids.forEach(id => {
        if (!document.getElementById(id).value.trim()) {
          document.getElementById('errorText').innerHTML = 'Niet alle velden zijn ingevuld.';
          errorFound = true;
        } 
      });
      if (document.getElementById('input-check-zakelijk').checked) {
        zakelijkIds.forEach(id => {
          if (!document.getElementById(id).value.trim()) {
            document.getElementById('errorText').innerHTML = 'Niet alle velden zijn ingevuld.';
            errorFound = true;
          }
        });
      }

      if (!errorFound){
        document.getElementById("submit-bestelling").disabled = true;
        document.getElementById("errorMelding").style.display = "none";

        document.getElementById("bestel-form").submit();
        document.getElementById("submit-bestelling").value = "Laden...";
      }else{
        document.getElementById("errorMelding").style.display = "block";
      }

    }


    updatePrices();
