    function formatPrice(price) {
      if (price > 0) {
        return `+ € ${price.toFixed(2).replace('.', ',')}`;
      } else if (price < 0) {
        return `- € ${Math.abs(price).toFixed(2).replace('.', ',')}`;
      } else {
        return ''; // Empty value for zero price
      }
    }

    function updatePrices() {
      document.getElementById('bat_1_price').innerHTML = formatPrice(bat_price_1 - bat_price_selected);
