      price_totaal_excl = price_totaal_korting / 1.21;
      price_totaal_btw = price_totaal_excl * 0.21;

      document.getElementById('final_price_excl').innerHTML = formatPrice(price_totaal_excl).replace(/\+/g, '');
      document.getElementById('final_price_btw').innerHTML = formatPrice(price_totaal_btw).replace(/\+/g, '');
      document.getElementById('final_price_incl').innerHTML = formatPrice(price_totaal_korting).replace(/\+/g, '');
      document.getElementById('final_price_aanbetaling').innerHTML = formatPrice(aanbetaling).replace(/\+/g, '');

      document.getElementById('spec_price').innerHTML = '<span class="summary indication direct-order-tekst">' + betaal_text;
    }


